{
  "name": "react-fast-scroll-pdf",
  "version": "1.0.0",
  "description": "Image-based PDF viewer using pdfjs",
  "author": "mothershipsoft",
  "license": "MIT",
  "repository": "mothershipsoft/react-fast-scroll-pdf",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rm ./dist/* -f && microbundle-crl --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "example": "npm run build && cd example && npm install && npm start"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "lodash": "^4.17.21",
    "pdfjs-dist": "^2.6.347",
    "react": "^17.0.1",
    "react-html-parser": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.7.22",
    "@types/react-html-parser": "^2.0.1",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "microbundle-crl": "^0.13.11",
    "react-scripts": "^3.4.0",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  }
}
